var int a = 0;
var int b = 0;
var point p1, p2;

var array[int](30) t0, t1;

class point {
    private attribute int x; 
    private attribute int y;
    static attribute int n0, n1;

    method void constructor(int x, int y) {
        this.x = x;
        this.y = y;
    }

    method void addX() {
        this.x = this.x + 1;
    }
}

main {
    // devrait alterner true - false - false - true

    point.n0 = 0;
    point.n1 = 0;
    print(point.n0===point.n1);
    print(point.n0=/=point.n1);
    point.n0 = 1;
    point.n1 = 2;
    print(point.n0===point.n1);
    print(point.n0=/=point.n1);


    p1 = new point(1, 1);
    p2 = new point(1, 1);
    print(p1===p2);
    print(p1=/=p2);
    p1.addX();
    print(p1===p2);
    print(p1=/=p2);

    print(t0 === t1);
    print(t0 =/= t1);
    t0[0] = 10;
    print(t0 === t1);
    print(t0 =/= t1);
